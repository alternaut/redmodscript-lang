{
  "name": "redmodscript-lang",
  "displayName": "REDmodScript Lang",
  "description": "Syntax highlighting for REDmod '.script' files",
  "repository": {
    "url": "https://github.com/alternaut/redmodscript-lang",
    "type": "git"
  },
  "license": "MIT",
  "homepage": "https://github.com/alternaut/redmodscript-lang",
  "publisher": "alternaut-dev",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.71.0"
  },
  "icon": "resources/icon_128.png",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "redmod-script",
        "aliases": [
          "REDmodScript",
          "redmod-script"
        ],
        "extensions": [
          ".script",
          ".ws"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "redmod-script",
        "scopeName": "source.redmod-script",
        "path": "./syntaxes/redmod-script.tmLanguage.json"
      }
    ]
  }
}
